        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DiveServlet</servlet-name>
        <url-pattern>/dive</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DiveListServlet</servlet-name>
        <url-pattern>/divelist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DiverServlet</servlet-name>
        <url-pattern>/diver</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DiveStatsServlet</servlet-name>
        <url-pattern>/divestats</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SpotServlet</servlet-name>
        <url-pattern>/spot</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
