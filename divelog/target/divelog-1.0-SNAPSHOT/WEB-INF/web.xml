    <servlet>
        <servlet-name>DiverServlet</servlet-name>
        <servlet-class>tsoha.divelog.control.DiverServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DiveStatsServlet</servlet-name>
        <servlet-class>tsoha.divelog.control.DiveStatsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>tsoha.divelog.control.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SpotServlet</servlet-name>
        <servlet-class>tsoha.divelog.control.SpotServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SpotslistServlet</servlet-name>
