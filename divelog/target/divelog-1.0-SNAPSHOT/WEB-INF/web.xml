    <servlet-mapping>
        <servlet-name>RemoveDiverServlet</servlet-name>
        <url-pattern>/RemoveDiverServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddDiveServlet</servlet-name>
        <url-pattern>/AddDiveServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddSpotServlet</servlet-name>
        <url-pattern>/AddSpotServlet</url-pattern>
    </servlet-mapping>
